{
  "manifest_version": 3,
  "name": "Key Press Monitor by Harvi",
  "version": "1.0",
  "description": "Displays the key pressed by the user on real-time",
  "permissions": ["activeTab", "scripting", "storage"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "index.html",
    "default_icon": "icon.png"
  }
}
